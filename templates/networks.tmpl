{{define "content"}}
<h2>Networks</h2>
<div class="table-responsive">
  <table class="table table-striped table-sm">
    <thead>
      <tr>
        <th>Number</th>
        <th>Run</th>
        <th>Network</th>
        <th>Elo</th>
        <th>Games</th>
        <th>Blocks</th>
        <th>Filters</th>
        <th>Time</th>
        <th>Ordo Elo</th>
      </tr>
    </thead>
    <tbody>
      {{range .networks}}
      <tr>
        <td>{{.number}}</td>
        <td>{{.training_id}}</td>
        <td><a href="/get_network?sha={{.sha}}" download="weights_run{{.training_id}}_{{.number}}.pb.gz">{{.short_sha}}</a></td>
        <td>{{.elo}}</td>
        <td>{{.games}}</td>
        <td>{{.blocks}}</td>
        <td>{{.filters}}</td>
        <td>{{.created_at}}</td>
        <td>{{.real_elo}}</td>
      </tr>
      {{end}}
    </tbody>
  </table>
</div>
{{end}}

{{define "scripts"}}
{{end}}
